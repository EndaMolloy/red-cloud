<div id="landing" class="content-section">
  <div class="circle">
    <canvas id="current-weather-icon" width="88" height="88"></canvas>
    <div class="container">
      <div class="row">
        <div class="col-3 edge-col no-padding-col">
          <div id="sun-rise">
            sunrise<br>{{toTime daily.data.0.sunriseTime}}
          </div>
        </div>
        <div class="col-6 no-padding-col">
          <div id="current-weather">{{currently.summary}}</div>
          <div id="current-temp">{{toCelcius currently.temperature}}°</div>
          <div id="feels-like">Feels like {{toCelcius currently.apparentTemperature}}°</div>
        </div>
        <div class="col-3 edge-col no-padding-col">
          <div id="sun-set">
            sunset<br>{{toTime daily.data.0.sunsetTime}}
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-12">
          <div id="rain-probability">
             Likelihood of {{daily.data.0.precipType}} today: <br>{{toPercent daily.data.0.precipProbability}}%
          </div>
        </div>
      </div>
    </div>
  </div>
  <div id="warning-alerts-container">
    <div id="warning-alerts">
      <div class="warning-headline">
        <img src="/img/{{weatherAlerts.0.status_color}}-warning.svg" width="27" height="25" alt="">
        <span id="warning-color">{{weatherAlerts.0.status_color}} Warning</span>
        <span id="warning-type"> - {{weatherAlerts.0.weather_type}}</span>
        <span id="warning-details"><a href="#">More</a></span>
      </div>
    </div>
  </div>
  <div id="next-hour-container">
